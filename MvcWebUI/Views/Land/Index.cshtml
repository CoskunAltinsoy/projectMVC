@model IEnumerable<Entities.Concrete.Land>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Arazi</h1>
<table class="table table-hover">
    <tr>
        <th>Id</th>
        <th>Emsal</th>
        <th>Template</th>
        <th>Tapu Durumu</th>
        <th>Arazi Tipi</th>
        <th>Takas</th>
        <th>Güncelle</th>
        <th>Sil</th>
        
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Precedent</td>
            <td>@item.</td>
            <td>@item.RoleName</td>
            <td>@item.Id</td>
            <td>@item.RoleName</td>
            <td><a class="btn btn-primary" href="/Role/Update/@item.Id">Güncelle</a></td>
            <td><a class="btn btn-danger" href="/Role/Delete/@item.Id">Sil</a></td>
        </tr>
    }
</table>
<br />
<a href="/Role/Add/" class= "btn btn-primary">Yeni Rol</a>


