        @Html.LabelFor(m => m.Property.ListingDate)
        @Html.TextBoxFor(m=>m.Property.ListingDate, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Property.AdvertNo)
        @Html.TextBoxFor(m=>m.Property.AdvertNo, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Property.AdvertState)
        @Html.TextBoxFor(m=>m.Property.AdvertState, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Property.Square)
        @Html.TextBoxFor(m=>m.Property.Square, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Land.Precedent)
        @Html.TextBoxFor(m=>m.Land.Precedent, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Land.Template)
        @Html.TextBoxFor(m=>m.Land.Template, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Land.DeedStatus)
        @Html.TextBoxFor(m=>m.Land.DeedStatus, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Land.LandType)
        @Html.TextBoxFor(m=>m.Land.LandType, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Land.ExchangeSatus)
        @Html.TextBoxFor(m=>m.Land.ExchangeSatus, new {@class="form-control"})
    </div>
