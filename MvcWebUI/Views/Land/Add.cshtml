@model MvcWebUI.Models.LandViewModel
@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Add</h1>
@using (Html.BeginForm("Add","Land",FormMethod.Post))
{
   
    <div class="form-group">
        @Html.LabelFor(m => m.Property.Price)
        @Html.TextBoxFor(m=>m.Property.Price, new {@class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Property.Picture)
        @Html.TextBoxFor(m=>m.Property.Picture, new {@class="form-control"})
